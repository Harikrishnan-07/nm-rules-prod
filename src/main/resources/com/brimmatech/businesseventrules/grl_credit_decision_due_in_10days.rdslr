package com.brimmatech.businesseventrules;

import java.lang.Number;

rule "grl_credit_decision_due_in_10days"
	dialect "mvel"
	agenda-group "credit_decision_due_10days"
	when
		For trigger event
		If credit decision due in ten days
	then
		Send notifications for the Loans with credit decision due in ten days
end
