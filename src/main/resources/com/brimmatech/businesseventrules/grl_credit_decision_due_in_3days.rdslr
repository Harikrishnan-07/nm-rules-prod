package com.brimmatech.businesseventrules;

import java.lang.Number;

rule "grl_credit_decision_due_in_3days"
	dialect "mvel"
	agenda-group "credit_decision_due_3days"
	when
		For trigger event
		If credit decision due in three day
	then
		Send notifications for the Loans with credit decision due in three days
end
